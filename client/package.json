{
  "name": "locadora-de-automoveis",
  "version": "1.0.0",
  "engines": {
    "node": "^20.19.0 || ^22.12.0 || ^24.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write .",
    "prebuild:prod": "node ./scripts/write-environment-prod.js",
    "build:prod": "npm run prebuild:prod && ng build --configuration production"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.10",
    "@angular/cdk": "^20.2.14",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@danielmoncada/angular-datetime-picker": "^20.0.0",
    "@jsverse/transloco": "^8.1.0",
    "@material/web": "^2.4.1",
    "angular-oauth2-oidc": "^20.0.2",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "fontawesome-free": "^1.0.4",
    "gs-buttons": "^3.1.0",
    "luxon": "^3.7.2",
    "ng-recaptcha-2": "^16.0.1",
    "ngx-mask": "^20.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.2",
    "@angular/cli": "^20.3.2",
    "@angular/compiler-cli": "^20.3.0",
    "@eslint/js": "^9.36.0",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.45.0"
  }
}
